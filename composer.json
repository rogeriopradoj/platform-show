{
    "name": "rogeriopradoj/platform-show",
    "description": "A easy way to get information of composer show --platform in a webpage",
    "keywords": ["composer", "show", "platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rogerio Prado de Jesus",
            "email": "rogeriopradoj@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.3",
        "vectorface/dunit": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "RogerioPradoJ\\": "src/rogeriopradoj/"
        },
        "psr-0": {
            "Composer": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RogerioPradoJ\\Test\\": "tests/"
        }
    }
}
